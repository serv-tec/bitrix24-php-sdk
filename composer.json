        "name": "bitrix24-php-sdk"
    },
    "support": {
        "issues": "https:\/\/github.com\/serv-tec\/Bitrix24-Php-Sdk\/issues",
        "wiki": "https:\/\/github.com\/serv-tec\/Bitrix24-Php-Sdk\/wiki",
        "source": "https:\/\/github.com\/serv-tec\/Bitrix24-Php-Sdk"
    },
    "replace": {
        "mesilov\/bitrix24-php-sdk": "*"
    }
}