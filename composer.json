{
    "name": "serv-tec\/bitrix24-php-sdk",
    "description": "A powerfull PHP library for the Bitrix24 REST API",
    "keywords": [
        "Bitrix24",
        "PHP",
        "REST",
        "API"
    ],
    "type": "library",
    "homepage": "https:\/\/github.com\/mesilov\/bitrix24-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxim Mesilov",
            "homepage": "https:\/\/github.com\/mesilov\/"
        },
        {
            "name": "serv-tec",
            "homepage": "https:\/\/github.com\/serv-tec",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "ext-json": "*",
        "ext-curl": "*",
        "psr\/log": "^1.0"
    },
    "require-dev": {
        "jakub-onderka\/php-parallel-lint": "0.9",
        "jakub-onderka\/php-console-highlighter": "~0.3",
        "phpunit\/phpunit": "~4.8",
        "phpunit\/phpunit-mock-objects": "2.3.0"
    },
    "autoload": {
        "classmap": [
            "src\/"
        ]
    },
    "scripts": {
        "test": [
            "parallel-lint . --exclude vendor --no-colors",
            "phpunit --colors=always --verbose"
        ]
    },
